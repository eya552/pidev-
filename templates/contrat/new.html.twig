      </nav>
    </div><!-- End Page Title -->



    
    


   <div class="card">
            <div class="card-body">
              <h5 class="card-title">Ajouter contrat</h5>

              <!-- Modifier bilan -->
                   {{ form_start(form,{'attr': {'novalidate': 'novalidate'}})}}
    <div class="quform-elements">
